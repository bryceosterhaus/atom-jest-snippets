{
  ".source.js, .source.jsx, .source.ts, .source.js-semantic": {
    "jsetup": {
      "description": "Sets up jest test file",
      "prefix": "jsetup",
      "leftLabelHTML": "<span style='color:#99424f'>Jest</span>",
      "body": "import $1 from './$1';\n\ndescribe('$1', () => {\n  test('$2', () => {\n      $3\n  });\n});\n"
    }
  }
}
